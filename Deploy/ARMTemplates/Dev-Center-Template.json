{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "name": {
            "type": "String"
        },
        "location": {
            "type": "String"
        },
        "tags": {
            "type": "Object"
        }
    },
    "resources": [
        {
            "type": "Microsoft.DevCenter/devcenters",
            "apiVersion": "2023-04-01",
            "name": "<devCenterName>",
            "location": "<location>",
            "tags": [
                {
                    "Environment": "DevBox",
                    "division": "IT"
                }
            ],
            "identity": {
                "type": "none"
            }
        }
    ]
}