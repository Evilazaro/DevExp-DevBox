{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {},
    "variables": {},
    "resources": [
        {
            "type": "Microsoft.Compute/galleries/images",
            "apiVersion": "2022-03-03",
            "name": "ContosoImageGallery/Win11-Manualv8-def",
            "location": "westus3",
            "properties": {
                "hyperVGeneration": "V2",
                "architecture": "x64",
                "features": [
                    {
                        "name": "SecurityType",
                        "value": "TrustedLaunch"
                    }
                ],
                "osType": "Windows",
                "osState": "Generalized",
                "identifier": {
                    "publisher": "Contoso",
                    "offer": "Contoso-Fabric",
                    "sku": "Contoso-sku8"
                },
                "recommended": {
                    "vCPUs": {
                        "min": 1,
                        "max": 16
                    },
                    "memory": {
                        "min": 1,
                        "max": 32
                    }
                }
            }
        }
    ]
}