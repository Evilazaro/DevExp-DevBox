{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "devcenters_Contoso_Dev_Center_name": {
            "defaultValue": "Contoso-Dev-Center",
            "type": "String"
        },
        "networkconnections_Contoso_Network_Connection_DevBox_externalid": {
            "defaultValue": "/subscriptions/9e946e16-45cd-4e6a-ad69-bb7c5e24c230/resourceGroups/Contoso-DevBox-rg/providers/Microsoft.DevCenter/networkconnections/Contoso-Network-Connection-DevBox",
            "type": "String"
        },
        "galleries_ContosoImageGallery_externalid": {
            "defaultValue": "/subscriptions/9e946e16-45cd-4e6a-ad69-bb7c5e24c230/resourceGroups/Contoso-DevBox-rg/providers/Microsoft.Compute/galleries/ContosoImageGallery",
            "type": "String"
        }
    },
    "variables": {},
    "resources": [
        {
            "type": "Microsoft.DevCenter/devcenters",
            "apiVersion": "2023-04-01",
            "name": "[parameters('devcenters_Contoso_Dev_Center_name')]",
            "location": "westus3",
            "tags": {
                "division": "IT",
                "Environment": "DevBox"
            },
            "identity": {
                "type": "UserAssigned",
                "userAssignedIdentities": {
                    "/subscriptions/9e946e16-45cd-4e6a-ad69-bb7c5e24c230/resourceGroups/Contoso-Base-Images-Engineers-rg/providers/Microsoft.ManagedIdentity/userAssignedIdentities/contosoIdentityIBuilderUserDevBox": {}
                }
            },
            "properties": {
                "devCenterUri": "https://2a44cbaf-da25-4a57-bf6c-7fcfb4267163-contoso-dev-center.westus3.devcenter.azure.com/"
            }
        },
        {
            "type": "Microsoft.DevCenter/devcenters/attachednetworks",
            "apiVersion": "2023-04-01",
            "name": "[concat(parameters('devcenters_Contoso_Dev_Center_name'), '/Contoso-Network-Connection-DevBox')]",
            "dependsOn": [
                "[resourceId('Microsoft.DevCenter/devcenters', parameters('devcenters_Contoso_Dev_Center_name'))]"
            ],
            "properties": {
                "networkConnectionId": "[parameters('networkconnections_Contoso_Network_Connection_DevBox_externalid')]"
            }
        },
        {
            "type": "Microsoft.DevCenter/devcenters/galleries",
            "apiVersion": "2023-04-01",
            "name": "[concat(parameters('devcenters_Contoso_Dev_Center_name'), '/ContosoImageGallery')]",
            "dependsOn": [
                "[resourceId('Microsoft.DevCenter/devcenters', parameters('devcenters_Contoso_Dev_Center_name'))]"
            ],
            "properties": {
                "galleryResourceId": "[parameters('galleries_ContosoImageGallery_externalid')]"
            }
        },
        {
            "type": "Microsoft.DevCenter/devcenters/galleries",
            "apiVersion": "2023-04-01",
            "name": "[concat(parameters('devcenters_Contoso_Dev_Center_name'), '/Default')]",
            "dependsOn": [
                "[resourceId('Microsoft.DevCenter/devcenters', parameters('devcenters_Contoso_Dev_Center_name'))]"
            ],
            "properties": {
                "galleryResourceId": "[resourceId('Microsoft.DevCenter/devcenters/galleries', parameters('devcenters_Contoso_Dev_Center_name'), 'Default')]"
            }
        },
        {
            "type": "Microsoft.DevCenter/devcenters/devboxdefinitions",
            "apiVersion": "2023-04-01",
            "name": "[concat(parameters('devcenters_Contoso_Dev_Center_name'), '/FrontEnd-Workstation')]",
            "location": "westus3",
            "dependsOn": [
                "[resourceId('Microsoft.DevCenter/devcenters', parameters('devcenters_Contoso_Dev_Center_name'))]",
                "[resourceId('Microsoft.DevCenter/devcenters/galleries', parameters('devcenters_Contoso_Dev_Center_name'), 'ContosoImageGallery')]"
            ],
            "properties": {
                "imageReference": {
                    "id": "[concat(resourceId('Microsoft.DevCenter/devcenters/galleries', parameters('devcenters_Contoso_Dev_Center_name'), 'ContosoImageGallery'), '/images/Win11EntBaseImageFrontEndEngineers/versions/1.0.0')]"
                },
                "sku": {
                    "name": "general_i_8c32gb256ssd_v2"
                },
                "hibernateSupport": "Enabled"
            }
        },
        {
            "type": "Microsoft.DevCenter/devcenters/devboxdefinitions",
            "apiVersion": "2023-04-01",
            "name": "[concat(parameters('devcenters_Contoso_Dev_Center_name'), '/VSCode')]",
            "location": "westus3",
            "dependsOn": [
                "[resourceId('Microsoft.DevCenter/devcenters', parameters('devcenters_Contoso_Dev_Center_name'))]",
                "[resourceId('Microsoft.DevCenter/devcenters/galleries', parameters('devcenters_Contoso_Dev_Center_name'), 'Default')]"
            ],
            "properties": {
                "imageReference": {
                    "id": "[concat(resourceId('Microsoft.DevCenter/devcenters/galleries', parameters('devcenters_Contoso_Dev_Center_name'), 'Default'), '/images/microsoftvisualstudio_visualstudioplustools_vs-2022-ent-general-win11-m365-gen2')]"
                },
                "sku": {
                    "name": "general_i_8c32gb256ssd_v2"
                },
                "hibernateSupport": "Enabled"
            }
        }
    ]
}